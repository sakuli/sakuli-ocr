{
  "name": "@sakuli/ocr",
  "version": "2.5.0",
  "description": "Enterprise OCR extension for Sakuli",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "copy": "node move-files.js ./out ./dist",
    "obfuscate": "javascript-obfuscator ./dist --output ./out --config obfuscate.js",
    "test": "jest",
    "build": "tsc",
    "build:release": "tsc && npm run obfuscate && npm run copy"
  },
  "keywords": [
    "sakuliPreset"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com//sakuli/sakuli-ocr.git"
  },
  "author": {
    "name": "Sven Hettwer",
    "email": "Sven.Hettwer@consol.de"
  },
  "contributors": [
    "Tim Keiner <tim.keiner@consol.de>",
    "Simon Hofmann <simon.hofmann@consol.de>",
    "Sven Hettwer <sven.hettwer@consol.de>",
    "Cliff Le Van <cliff.le.van@consol.de>"
  ],
  "dependencies": {
    "@nut-tree/nut-js": "1.6.0",
    "@sakuli/core": "2.5.0",
    "@sakuli/legacy": "2.5.0",
    "canvas": "2.6.1",
    "jsdom": "16.4.0"
  },
  "devDependencies": {
    "@sakuli/legacy-types": "2.4.0",
    "@types/jest": "27.0.1",
    "@types/mock-fs": "4.13.0",
    "@types/node": "12.12.34",
    "@types/selenium-webdriver": "4.0.10",
    "fs-extra": "9.0.1",
    "husky": "4.2.5",
    "javascript-obfuscator": "2.8.1",
    "jest": "27.2.0",
    "md5-file": "5.0.0",
    "mock-fs": "4.13.0",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "sneer": "1.0.1",
    "ts-jest": "27.0.5",
    "typescript": "4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
