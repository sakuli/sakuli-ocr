{
  "name": "@sakuli/ocr",
  "version": "2.4.0",
  "description": "Enterprise OCR extension for Sakuli",
  "main": "dist/index.js",
  "typings": "dist/index.d.js",
  "scripts": {
    "publish:next": "npm run obfuscate && npm run copy && npm run version:next && npm publish --access restricted --tag next",
    "publish:release": "npm run obfuscate && npm run copy && npm version --no-git-tag-version ${SAKULI_VERSION} && npm publish --access restricted",
    "version:next": "npm version --no-git-tag-version $(npm --no-git-tag version patch)-next.$(date +%s)",
    "copy": "node move-files.js ./out ./dist",
    "obfuscate": "javascript-obfuscator ./dist --output ./out --config obfuscate.js",
    "test": "jest",
    "build": "tsc"
  },
  "keywords": [
    "sakuliPreset"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com//sakuli/sakuli-ocr.git"
  },
  "author": {
    "name": "Sven Hettwer",
    "email": "Sven.Hettwer@consol.de"
  },
  "contributors": [
    "Tim Keiner <tim.keiner@consol.de>",
    "Simon Hofmann <simon.hofmann@consol.de>",
    "Sven Hettwer <sven.hettwer@consol.de>",
    "Cliff Le Van <cliff.le.van@consol.de>"
  ],
  "dependencies": {
    "@sakuli/core": "2.4.0"
  },
  "devDependencies": {
    "@types/node": "12.12.34",
    "@types/jest": "26.0.15",
    "javascript-obfuscator": "2.8.1",
    "typescript": "3.8.3",
    "jest": "26.6.3",
    "ts-jest": "26.4.4",
    "fs-extra": "9.0.1"
  }
}
